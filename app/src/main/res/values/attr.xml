        <attr name="pan_num"  format="integer"/>
        <attr name="names" format="reference"/>
        <attr name="icons"  format="reference" />
        <attr name="rotate_radius" format="dimension"/>
        <attr name="rotate_color" format="color"/>
        <attr name="rotate_color_dark" format="color" />
